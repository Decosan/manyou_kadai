<h1>User Info.</h1>
<div class='row'>
  <div class='col-xs-5'>
    <% if @user.admin? %>
      <%= link_to "Admin Page",admin_users_path,class:'btn btn-primary btn-sm' %>
    <% end %>
    <div class="panel panel-default">
      <div class="panel-heading">
        USER: <%= @user.name %>
      </div>
      <div class="panel-body">
        Registerd: <%= @user.created_at %>
      </div>
    </div>
  </div>  
</div>  